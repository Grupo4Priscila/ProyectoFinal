<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#11ac26"
    android:id="@+id/relativeLayout"
    tools:context="cl.ucn.disc.dam.autolog.activity.PopActivity">>

    <TextView
        android:id="@+id/pop_anio"
        android:layout_width="76dp"
        android:layout_height="36dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="anio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_patente" />

    <TextView
        android:id="@+id/pop_anexo"
        android:layout_width="74dp"
        android:layout_height="28dp"
        android:layout_marginEnd="56dp"
        android:layout_marginTop="12dp"
        android:text="anexo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_tipo" />

    <TextView
        android:id="@+id/pop_persona"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Persona:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_descripcion"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_rut"
        android:layout_width="110dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="rut"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_nombre"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_correo"
        android:layout_width="119dp"
        android:layout_height="27dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="correo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_rut"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_marca"
        android:layout_width="111dp"
        android:layout_height="31dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="4dp"
        android:text="marca"
        app:layout_constraintStart_toEndOf="@+id/pop_anio"
        app:layout_constraintTop_toBottomOf="@+id/pop_modelo"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_color"
        android:layout_width="96dp"
        android:layout_height="28dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pop_marca"
        app:layout_constraintTop_toBottomOf="@+id/pop_modelo"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_localizacion"
        android:layout_width="101dp"
        android:layout_height="36dp"
        android:layout_marginEnd="28dp"
        android:layout_marginTop="16dp"
        android:text="localizacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_anexo" />

    <TextView
        android:id="@+id/pop_descripcion"
        android:layout_width="311dp"
        android:layout_height="39dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="descripcion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_marca"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_tipo"
        android:layout_width="96dp"
        android:layout_height="32dp"
        android:layout_marginEnd="36dp"
        android:layout_marginTop="12dp"
        android:text="tipo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_cargo" />

    <TextView
        android:id="@+id/pop_cargo"
        android:layout_width="84dp"
        android:layout_height="37dp"
        android:layout_marginEnd="52dp"
        android:layout_marginTop="16dp"
        android:text="cargo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_persona" />

    <TextView
        android:id="@+id/pop_patente"
        android:layout_width="172dp"
        android:layout_height="31dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="patente"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_nombre"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="nombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_persona"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pop_modelo"
        android:layout_width="122dp"
        android:layout_height="36dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="modelo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/pop_fono"
        android:layout_width="117dp"
        android:layout_height="28dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="fono"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_correo"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnRegistrar"
        android:layout_width="344dp"
        android:layout_height="36dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Registrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_localizacion"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Vehiculo:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

</android.support.constraint.ConstraintLayout>